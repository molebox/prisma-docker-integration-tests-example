{
  "name": "prisma-docker-integration-tests-example",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Rich Haines",
  "license": "MIT",
  "scripts": {
    "migrate": "npx prisma migrate dev --name init",
    "docker:build": "docker-compose build --no-cache",
    "docker:create": "docker-compose up -d",
    "docker:down": "docker compose down",
    "seed": "node --loader ts-node/esm ./prisma/seed.ts",
    "bootstrap": "yarn docker:create && yarn migrate && yarn seed",
    "test": "yarn bootstrap && jest -i && yarn docker:down"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "dotenv-cli": "^4.0.0",
    "jest": "^26.6.3",
    "prisma": "^2.21.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@prisma/client": "^2.21.2"
  }
}
